{
  "name": "hydejack",
  "version": "6.0.0-alpha",
  "description": "",
  "scripts": {
    "clean": "rm -f public/js/hydejack.js & rm -f public/js/hydejack.min.js",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "build": "cross-env NODE_ENV=production browserify _js/src/index.js -g babelify | uglifyjs -c -m -o public/js/hydejack-$npm_package_version.js",
    "watch": "watchify -d _js/src/index.js -g babelify -o public/js/hydejack-$npm_package_version.js",
    "lint": "eslint _js/src",
    "dev": "npm run watch",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build",
    "resume": "json2yaml resume.json > _data/resume.yml",
    "unresume": "yaml2json _data/resume.yml > resume.json"
  },
  "author": "Florian Klampfer <f.klampfer@gmail.com> (https://qwtel.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "7.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-es2016": "6.22.0",
    "babelify": "7.3.0",
    "browserify": "14.1.0",
    "cross-env": "3.2.4",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "json2yaml": "1.1.0",
    "uglify-js": "2.8.16",
    "watchify": "3.9.0",
    "yaml2json": "1.0.2"
  },
  "dependencies": {
    "core-js": "2.4.1",
    "fg-loadcss": "1.3.1",
    "katex": "0.7.1",
    "rxjs": "5.2.0",
    "web-animations-js": "2.2.2",
    "y-drawer": "^5.0.0",
    "y-push-state": "^5.0.0"
  }
}
